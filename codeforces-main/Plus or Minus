import java.util.*;

public class Mavenproject1 {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n=in.nextInt();
        for (int i = 0; i < n; i++) {
            int a=in.nextInt();
            int b=in.nextInt();
            int c=in.nextInt();
            if(a+b==c)
                System.out.println("+");
            else
                System.out.println("-");
        }
    }
}     
